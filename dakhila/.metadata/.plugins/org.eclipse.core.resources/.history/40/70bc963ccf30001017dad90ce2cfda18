<?php
require_once "db.inc";
require_once "vidyalaya.inc";

$students = GetAllData();


foreach ($students as $id => $student) {
	if ($student->IsEnrolled) {
		$zip = $student->family->address->zipcode;
		if (array_key_exists($zip, $tata)) {
			$tata[$zip]++;
		} else {
			$tata[$zip]=1;
		}
	}
	
	
}
ksort($tata);
foreach ($tata as $zip => $number) {
	print "zip $zip has  $number\n";
}

?>