<?php
require_once "db.inc";
require_once "vidyalaya.inc";

$students = GetAllData();


foreach ($students as $id => $student) {
	if ($student->IsEnrolled) {
		$zip = $student->family->address->zip;
		$city = $student->family->address->city;
		if (array_key_exists($zip, $tata)) {
			$tata[$zip]++;
		} else {
			$tata[$zip]=1;
			$town{$zip} = $city;
		}
	}
	
	
}
ksort($tata);
foreach ($tata as $zip => $number) {
	print "$number  $zip, $town[$zip] \n";
}

?>